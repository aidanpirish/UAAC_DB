<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Available Cats</div>
  <!-- Table -->
  <table class="table table-striped">
    <th>ID</th>
    <th>Name</th>
    <th>Breed</th>
    <th>Spayed Or Neutered</th>

    {{#each available}}
    <tr>
      <td>{{Animal_ID}}</td>
      <td>{{Animal_Name}}</td>
      <td>{{Breed}}</td>
      <td>{{Spayed_Or_Neutered}}</td>
    </tr>
    {{/each}}
  </table>
</div>
